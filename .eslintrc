{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  
  "env": {
    "mocha": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "plugins": [],
  "rules": {
    "import/prefer-default-export": 0,
    "no-unused-vars": [2, { "argsIgnorePattern": "^_" }],
    "react/require-extension": 0,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "no-await-in-loop": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": ["off", "windows"],
    "max-len": 0,
    "no-console": "off"
  }
}